                  <Image url="@Images/FilterButtonIcon.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" style="-fx-background-radius: 100;">
         <graphic>
            <ImageView fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/two-left.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="btnPlay" layoutX="66.0" layoutY="14.0" mnemonicParsing="false" style="-fx-background-radius: 100;" prefWidth="40.0" prefHeight="40.0" onAction="#playSong">
