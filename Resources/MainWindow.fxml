            <ImageView fx:id="playPauseImageView" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/one-right.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="118.0" layoutY="14.0" mnemonicParsing="false" style="-fx-background-radius: 100;" onAction="#nextSong">
