      <Button layoutX="20.0" layoutY="535.0" mnemonicParsing="false" text="New..." />
      <Button layoutX="106.0" layoutY="535.0" mnemonicParsing="false" text="Edit..." />
      <Button layoutX="194.0" layoutY="535.0" mnemonicParsing="false" text="Delete" />
      <Button layoutX="274.0" layoutY="535.0" mnemonicParsing="false">
