                  <Image url="@Images/two-right.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <ListView layoutX="274.0" layoutY="100.0" prefHeight="435.0" prefWidth="140.0" />
      <TableView layoutX="18.0" layoutY="100.0" prefHeight="435.0" prefWidth="228.0">
